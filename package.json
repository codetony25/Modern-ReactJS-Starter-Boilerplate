{
  "name": "react-webpack-starter",
  "version": "1.0.0",
  "description": "",
  "main": "index.jsx",
  "scripts": {
    "start": "better-npm-run start",
    "compile": "better-npm-run compile",
    "deploy": "better-npm-run deploy",
    "clean": "rm -rf dist",
    "lint": "better-npm-run lint",
    "lint:app:jsx": "eslint ./app/**/**/*.jsx || true",
    "lint:app:js": "eslint ./app/**/**/*.js || true",
    "lint:node": "eslint . || true"
  },
  "betterScripts": {
    "start": {
      "command": "babel-node bin/server",
      "env": {
        "NODE_ENV": "development"
      }
    },
    "compile": {
      "command": "babel-node bin/compile",
      "env": {
        "NODE_ENV": "production"
      }
    },
    "deploy": {
      "command": "npm run clean && npm run compile",
      "env": {
        "NODE_ENV": "production"
      }
    },
    "lint": {
      "command": "npm run lint:app:jsx && npm run lint:node && npm run lint:app:js"
    }
  },
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "autoprefixer": "^6.3.6",
    "babel-cli": "^6.7.5",
    "babel-core": "^6.7.6",
    "babel-eslint": "^6.0.2",
    "babel-loader": "^6.2.4",
    "babel-plugin-add-module-exports": "^0.1.2",
    "babel-plugin-react-transform": "^2.0.2",
    "babel-plugin-transform-react-constant-elements": "^6.5.0",
    "babel-plugin-transform-react-remove-prop-types": "^0.2.5",
    "babel-plugin-transform-runtime": "^6.7.5",
    "babel-plugin-typecheck": "^3.8.0",
    "babel-preset-es2015": "^6.6.0",
    "babel-preset-react": "^6.5.0",
    "babel-preset-react-hmre": "^1.1.1",
    "babel-preset-stage-0": "^6.5.0",
    "babel-register": "^6.7.2",
    "better-npm-run": "0.0.8",
    "css-loader": "^0.23.1",
    "debug": "^2.2.0",
    "eslint": "^2.8.0",
    "eslint-config-standard": "^5.1.0",
    "eslint-config-standard-react": "^2.3.0",
    "eslint-loader": "^1.3.0",
    "eslint-plugin-promise": "^1.1.0",
    "eslint-plugin-react": "^4.3.0",
    "eslint-plugin-standard": "^1.3.2",
    "express": "^4.13.4",
    "extract-text-webpack-plugin": "^1.0.1",
    "node-sass": "^3.4.2",
    "postcss-loader": "^0.8.2",
    "poststylus": "^0.2.3",
    "react-transform-catch-errors": "^1.0.2",
    "react-transform-hmr": "^1.0.4",
    "rucksack-css": "^0.8.5",
    "style-loader": "^0.13.1",
    "stylus-loader": "^2.0.0",
    "webpack": "^1.12.14",
    "webpack-dev-middleware": "^1.6.1",
    "webpack-dev-server": "^1.14.1",
    "webpack-hot-middleware": "^2.10.0"
  },
  "dependencies": {
    "body-parser": "^1.15.0",
    "html-loader": "^0.4.3",
    "html-webpack-plugin": "^2.15.0",
    "json-loader": "^0.5.4",
    "react": "^15.0.1",
    "react-dom": "^15.0.1",
    "redbox-react": "^1.2.3",
    "sass-loader": "^3.2.0",
    "webpack": "^1.13.0"
  }
}
