{
  "name": "modern-reactjs-starter-boilerplate",
  "version": "1.0.0",
  "description": "",
  "main": "index.jsx",
  "scripts": {
    "start": "better-npm-run start",
    "compile": "better-npm-run compile",
    "deploy": "better-npm-run deploy",
    "clean": "rm -rf dist && npm cache clean",
    "lint": "better-npm-run lint",
    "lint:jsx": "eslint ./app/**/**/*.jsx || true",
    "lint:js": "eslint ./app/**/**/*.js || true",
    "lint:node": "eslint . || true",
    "lint:css": "./node_modules/.bin/stylelint '**/*.css' || true"
  },
  "betterScripts": {
    "start": {
      "command": "babel-node bin/server",
      "env": {
        "NODE_ENV": "development",
        "DEBUG": "app:*"
      }
    },
    "compile": {
      "command": "babel-node bin/compile",
      "env": {
        "NODE_ENV": "production",
        "DEBUG": "app:*"
      }
    },
    "deploy": {
      "command": "npm run clean && npm run compile",
      "env": {
        "NODE_ENV": "production",
        "DEBUG": "app:*"
      }
    },
    "lint": {
      "command": "npm run lint:jsx && npm run lint:node && npm run lint:js && npm run lint:css"
    }
  },
  "author": "Tony Tai Nguyen",
  "license": "ISC",
